#Create the percent loss
firedata$PercentLoss <- as.numeric(firedata$Pre_Inci_Value) / as.numeric(firedata$Total_Loss)
#filter out non fires
firedata <- filter(firedata, Incident.Type != "Not a Fire")
head(firedata)
